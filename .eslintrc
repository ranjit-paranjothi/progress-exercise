{
	"parser": "babel-eslint",
	"env": {
	    "browser": true,
        "es6": true,
	    "node": true,
	    "jest": true
  	},
    "rules": {
    	"react/jsx-uses-react": 1,
    	"react/jsx-uses-vars": 1,
    	"no-unused-vars": ["error", { "vars": "all", "args": "none", "ignoreRestSiblings": false }],
    	"no-console":0,
        "eqeqeq": "off",
        "curly": "warn",
        "quotes": ["warn", "double"],
         "semi": 2
    },
    "extends":"eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "eslint-plugin-react",
    ]
}